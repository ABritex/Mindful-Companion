{
    // Windows Settings
    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "all",
    "window.title": "${activeEditorShort}${separator}${rootName}",
    "window.titleSeparator": " ・☣・",
    "window.density.editorTabHeight": "compact",
    // "window.titleBarStyle": "custom",
    "window.commandCenter": false,
    "window.menuBarVisibility": "toggle",
    "window.reopenFolders": "all",
    "files.hotExit": "onExit",
    // Workbench Settings
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.restoreViewState": false,
    "workbench.editor.sharedViewState": false,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.tabActionLocation": "right",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editorAssociations": {
        "*.sqlite": "sqlite-viewer.view",
        "*.bin": "default"
    },
    "workbench.layoutControl.type": "menu",
    "workbench.list.smoothScrolling": true,
    "workbench.panel.defaultLocation": "right",
    "workbench.reduceMotion": "off",
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "bottom",
    "workbench.colorCustomizations": {
        // VS Code Dark Modern Text Colors
        "foreground": "#cccccc",
        "editor.foreground": "#cccccc",
        "descriptionForeground": "#ccccccb3",
        // Editor text colors
        "editorLineNumber.foreground": "#858585",
        "editorLineNumber.activeForeground": "#c6c6c6",
        "editorCursor.foreground": "#ffffff",
        // Sidebar and panel text
        "sideBar.foreground": "#cccccc",
        "sideBarTitle.foreground": "#cccccc",
        "sideBarSectionHeader.foreground": "#cccccc",
        // Activity bar text
        "activityBar.foreground": "#ffffff",
        "activityBarBadge.foreground": "#ffffff",
        // Tab text colors
        "tab.activeForeground": "#ffffff",
        "tab.inactiveForeground": "#ffffff80",
        "tab.unfocusedActiveForeground": "#ffffff80",
        "tab.unfocusedInactiveForeground": "#ffffff40",
        // Panel text
        "panelTitle.activeForeground": "#e7e7e7",
        "panelTitle.inactiveForeground": "#e7e7e799",
        // Status bar text
        "statusBar.foreground": "#ffffff",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.noFolderForeground": "#ffffff",
        // Title bar text
        "titleBar.activeForeground": "#cccccc",
        "titleBar.inactiveForeground": "#cccccc99",
        // Terminal text
        "terminal.foreground": "#cccccc",
        // Input and dropdown text
        "input.foreground": "#cccccc",
        "input.placeholderForeground": "#cccccc80",
        "dropdown.foreground": "#cccccc",
        // Button text
        "button.foreground": "#ffffff",
        // List text colors
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionForeground": "#ffffff",
        "list.focusForeground": "#ffffff",
        "list.hoverForeground": "#ffffff",
        "list.highlightForeground": "#18a3ff",
        "list.invalidItemForeground": "#b89500",
        // Badge text
        "badge.foreground": "#ffffff",
        // Notification text
        "notifications.foreground": "#cccccc",
        "notificationCenterHeader.foreground": "#cccccc",
        "notificationLink.foreground": "#3794ff",
        // Extension text
        "extensionButton.prominentForeground": "#ffffff",
        // Picker text
        "pickerGroup.foreground": "#3794ff",
        // Text document colors
        "textLink.foreground": "#3794ff",
        "textLink.activeForeground": "#3794ff",
        "textPreformat.foreground": "#d7ba7d",
        "textSeparator.foreground": "#ffffff2e",
        // Editor widget text
        "editorWidget.foreground": "#cccccc",
        "editorSuggestWidget.foreground": "#cccccc",
        "editorSuggestWidget.highlightForeground": "#18a3ff",
        // Git decoration text colors
        "gitDecoration.modifiedResourceForeground": "#e2c08d",
        "gitDecoration.deletedResourceForeground": "#c74e39",
        "gitDecoration.untrackedResourceForeground": "#73c991",
        "gitDecoration.ignoredResourceForeground": "#8c8c8c",
        "gitDecoration.conflictingResourceForeground": "#e4676b",
        // Error, warning, info text colors
        "errorForeground": "#f85149",
        "editorError.foreground": "#f85149",
        "editorWarning.foreground": "#ff8c00",
        "editorInfo.foreground": "#3794ff",
        "editorHint.foreground": "#eeeeeeb3"
    },
    // Editor Settings
    "editor.folding": true,
    "editor.glyphMargin": true,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.wordWrap": "off",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "never"
    },
    "editor.colorDecorators": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "line-thin",
    "editor.detectIndentation": false,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "Geist Mono, JetBrains Mono, Fira Code",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.fontSize": 10,
    "editor.fontVariations": true,
    "editor.fontWeight": "normal",
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": "active",
    "editor.guides.highlightActiveBracketPair": true,
    "editor.hover.above": false,
    "editor.inlayHints.enabled": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.insertSpaces": true,
    "editor.lightbulb.enabled": "off",
    //"editor.lineHeight": 20,
    "editor.linkedEditing": true,
    "editor.matchBrackets": "never",
    "editor.maxTokenizationLineLength": 99999999,
    "editor.mouseWheelScrollSensitivity": 1.5,
    "editor.multiCursorModifier": "alt",
    "editor.occurrencesHighlight": "off",
    "editor.padding.top": 8,
    "editor.quickSuggestions": {
        "comments": "on",
        "other": "on",
        "strings": "on"
    },
    "editor.renderControlCharacters": true,
    "editor.renderFinalNewline": "on",
    "editor.renderLineHighlight": "gutter",
    "editor.renderWhitespace": "none",
    "editor.scrollbar.horizontal": "auto",
    "editor.scrollbar.vertical": "auto",
    "editor.selectionHighlight": true,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 5,
    "editor.stickyTabStops": false,
    "editor.suggest.insertMode": "replace",
    "editor.suggest.preview": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordBasedSuggestions": "matchingDocuments",
    "editor.lineNumbers": "off",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    // Miscellaneous Settings
    "files.exclude": {
        "**/__pycache__": true
    },
    "editor.minimap.enabled": false,
    "workbench.layoutControl.enabled": false,
    //   "editor.defaultFormatter": "esbenp.prettier-vscode",
    //   "[javascript]": {
    //     "editor.defaultFormatter": "esbenp.prettier-vscode"
    //   },
    "terminal.integrated.env.linux": {},
    "javascript.updateImportsOnFileMove.enabled": "always",
    "security.workspace.trust.untrustedFiles": "open",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "powermode.enabled": true,
    "powermode.presets": "flames",
    "powermode.shake.enabled": false,
    "editor.guides.indentation": false,
    "breadcrumbs.enabled": false,
    "scm.diffDecorations": "none",
    "editor.hover.delay": 1500,
    "editor.hover.enabled": true,
    "git.decorations.enabled": false,
    "editor.overviewRulerBorder": false,
    "problems.decorations.enabled": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "workbench.colorTheme": "Community Material Theme Ocean",
    "window.titleBarStyle": "native",
    "window.customTitleBarVisibility": "never",
    "explorer.confirmPasteNative": false,
}